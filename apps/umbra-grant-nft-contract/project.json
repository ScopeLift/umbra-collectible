{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/umbra-grant-nft-contract/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge build --root apps/umbra-grant-nft-contract"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge install --root apps/umbra-grant-nft-contract"
      }
    },
    "add": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge install --root apps/umbra-grant-nft-contract {args.package} --no-commit"
      }
    },
    "deployLocal": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge script script/Collectible.s.sol:DeployCollectible --fork-url http://localhost:8545 --private-key $PRIVATE_KEY0 --broadcast",
        "cwd": "./apps/umbra-grant-nft-contract"
      }
    },
    "deployMumbai": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge script script/Collectible.s.sol:DeployCollectible --fork-url https://matic-mumbai.chainstacklabs.com --private-key 80bddf041314c0634043f8bec30fdd635bd0162f56303406e8ac83633d3cc541 --broadcast",
        "cwd": "./apps/umbra-grant-nft-contract"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge test --root apps/umbra-grant-nft-contract"
      }
    },
    "trace": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge test -vvvv --root apps/umbra-grant-nft-contract"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "forge clean --root apps/umbra-grant-nft-contract"
      }
    }
  },
  "tags": []
}
